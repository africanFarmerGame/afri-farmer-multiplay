package uk.ac.sussex.view.components {		import uk.ac.sussex.model.valueObjects.Game;	import flash.display.MovieClip;		public class JoinExistingGame extends MovieClip {		private var scrollingList:ScrollingList;		public function JoinExistingGame() {			// constructor code			//Needs a join button, if nothing else. 			scrollingList = new ScrollingList(300, 400);			this.addChild(scrollingList);			var myGameButton:GameButton = new GameButton("Join game");			myGameButton.y = scrollingList.height + 10;			this.addChild(myGameButton);		}		public function addGameItem(game:Game):void {			//scrollingList.addTextItem(gameName);			var gameItem:GameListItem = new GameListItem();			gameItem.setDisplayName(game.getName());			gameItem.setLocked(false);			gameItem.setItemID(game.getId().toString());			scrollingList.addItem(gameItem);		}		public function getCurrentValue():String {			return scrollingList.getCurrentValue();		}	}	}