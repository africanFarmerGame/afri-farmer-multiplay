package uk.ac.sussex.view.components {	import flash.text.*;	import flash.display.MovieClip;		public class LogoutButton extends MovieClip{		private static const DEFAULT_Y_SPACER:uint = 5;		private static const DEFAULT_X_SPACER:uint = 10;				public function LogoutButton() {			var adFormat:TextFormat = new TextFormat();			var button:GameButton = new GameButton("Logout");			var smartFoxAd:TextField = new TextField();						button.x = DEFAULT_X_SPACER;			button.y = DEFAULT_Y_SPACER;			this.addChild(button);						adFormat.font = "Arial";			adFormat.size = 10;			adFormat.bold = true;			adFormat.align = TextFormatAlign.RIGHT;						smartFoxAd.defaultTextFormat = adFormat;			smartFoxAd.height = 23;			smartFoxAd.width = button.width * 2;			smartFoxAd.selectable = false;			smartFoxAd.border = false;			smartFoxAd.textColor = 0x09063A;			smartFoxAd.wordWrap = false;			smartFoxAd.x = DEFAULT_X_SPACER;			smartFoxAd.y = DEFAULT_Y_SPACER + button.y+ button.height;			smartFoxAd.text = "Powered by SmartFoxServer";						this.addChild(smartFoxAd);		}	}	}