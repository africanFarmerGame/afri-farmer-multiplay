package uk.ac.sussex.view.components {	import flash.display.MovieClip;		public class AssetIcon extends MovieClip {		public static const NONE:String = "NONE";		public function AssetIcon() {			// constructor code		}		public function setType(type:String, subtype:String = null):void{			switch (type) {				case "FOOD":				case "CROP":					this.selectCrop(subtype);					break;				case "FERTILISER":					this.selectFertiliser(subtype);					break;				case "HERBICIDE":					this.gotoAndStop("herbicide");					break;				case "INSECTICIDE":					this.gotoAndStop("pesticide");					break;				case "DIET":					this.gotoAndStop("diet");					break;				case "ALLOCATION":					this.gotoAndStop("allocation");					break;				case "LAND":					this.gotoAndStop("land");					break;				case NONE:					this.gotoAndStop("notrequired");					break;				case "OTHER":				default:					this.selectOther(subtype);							}		}		private function selectCrop(name:String):void {			switch(name.toUpperCase()) {				case "MAIZE":					this.gotoAndStop("local maize");					break;				case "COTTON":					this.gotoAndStop("cotton-seed");					break;				case "CASSAVA":					this.gotoAndStop("cassava");					break;				case "GRAIN":					this.gotoAndStop("sorghum");					break;				case "HORTICULTURE":					this.gotoAndStop("horticulture");					break;				default:					this.gotoAndStop(1);					break;			}		}		private function selectOther(name:String):void {			trace("AssetIcon sez: The asset name is " + name);			switch(name.toUpperCase()) {				case "CASH":					this.gotoAndStop("cash");					break;				case "SPRAYKIT":					this.gotoAndStop("spray kit");					break;				case "VOUCHERS":					this.gotoAndStop("vouchers");					break;				case "CROPSTORE":					this.gotoAndStop("granary");					break;				case "COTTON":					this.gotoAndStop("cotton");					break;				default: 					this.gotoAndStop("notrequired");			}		}		private function selectFertiliser(subtype:String):void {			switch(subtype.toUpperCase()) {				case "ORGANIC":					this.gotoAndStop("manure");					break;				case "INORGANIC":				default:					this.gotoAndStop("fertilizer");					break;			}		}	}	}