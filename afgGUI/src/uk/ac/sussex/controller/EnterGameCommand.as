package uk.ac.sussex.controller {	import uk.ac.sussex.serverhandlers.GameHandlers;	import uk.ac.sussex.model.IncomingDataProxy;	import uk.ac.sussex.model.valueObjects.requestParams.*;	import org.puremvc.as3.multicore.interfaces.*;	import org.puremvc.as3.multicore.patterns.command.*;	import uk.ac.sussex.model.RequestProxy;	/**	 * @author em97	 */	public class EnterGameCommand extends SimpleCommand {		override public function execute(note:INotification): void{			trace("EnterGameCommand sez: I've been fired - " + note.getName());			var requestName:String = GameHandlers.ENTER_GAME;			var enterGameRequest:RequestProxy = facade.retrieveProxy(requestName+RequestProxy.NAME) as RequestProxy;			if(enterGameRequest == null){				enterGameRequest = new RequestProxy(requestName);				facade.registerProxy(enterGameRequest);			}						//Ok, we'll register the incoming data listeners here too. Just take 'em out when we hear one!			var enterGameSuccess:IncomingDataProxy = new IncomingDataProxy(GameHandlers.ENTER_GAME_SUCCESS, GameHandlers.ENTER_GAME_SUCCESS);			enterGameSuccess.addDataParam(new DataParamString("message"));			facade.registerProxy(enterGameSuccess);						var enterGameError:IncomingDataProxy = new IncomingDataProxy(GameHandlers.ENTER_GAME_ERROR, GameHandlers.ENTER_GAME_ERROR);			enterGameError.addDataParam(new DataParamString("message"));			facade.registerProxy(enterGameError);						enterGameRequest.sendRequest();								 		}	}}