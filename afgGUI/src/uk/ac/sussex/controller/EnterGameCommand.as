DataProxy = new IncomingDataProxy(GameHandlers.ENTER_GAME_ERROR, GameHandlers.ENTER_GAME_ERROR);			enterGameError.addDataParam(new DataParamString("message"));			facade.registerProxy(enterGameError);						enterGameRequest.sendRequest();								 		}	}}