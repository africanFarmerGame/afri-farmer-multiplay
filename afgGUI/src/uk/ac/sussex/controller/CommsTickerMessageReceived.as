package uk.ac.sussex.controller {	import org.puremvc.as3.multicore.interfaces.INotification;	import org.puremvc.as3.multicore.patterns.command.*;		import uk.ac.sussex.model.valueObjects.IncomingData;	import uk.ac.sussex.model.TickerProxy;		public class CommsTickerMessageReceived extends SimpleCommand {		override public function execute(note:INotification):void {			var incoming:IncomingData = note.getBody() as IncomingData;			var tickerMessage:String = incoming.getParamValue("Message") as String;			var tickerProxy:TickerProxy = facade.retrieveProxy(TickerProxy.NAME) as TickerProxy;			if(tickerProxy == null){				tickerProxy = new TickerProxy();				facade.registerProxy(tickerProxy);			}			tickerProxy.setTickerMessage(tickerMessage);		}	}	}