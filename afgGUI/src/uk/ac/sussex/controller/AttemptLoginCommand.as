package uk.ac.sussex.controller {	import org.puremvc.as3.multicore.patterns.command.*;	import org.puremvc.as3.multicore.interfaces.*;		import uk.ac.sussex.states.LoginGameState;	import uk.ac.sussex.model.valueObjects.Form;	import uk.ac.sussex.model.ServerProxy;	public class AttemptLoginCommand extends SimpleCommand{		override public function execute( note:INotification ):void {			trace("AttemptLoginCommand sez: I've been triggered.");			var form:Form = note.getBody() as Form;			if(form != null){				var serverProxy:ServerProxy = facade.retrieveProxy(ServerProxy.NAME) as ServerProxy;				var password:String = form.getFieldValue(LoginGameState.FORM_FIELD_PASSWORD);				var username:String = form.getFieldValue(LoginGameState.FORM_FIELD_USERNAME);				serverProxy.login(username,password);			} else {				trace("AttemptLoginCommand sez: There was a problem - the form data was null");			}					}	}	}