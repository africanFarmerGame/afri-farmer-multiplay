package uk.ac.sussex.controller {	import org.puremvc.as3.multicore.interfaces.*;	import org.puremvc.as3.multicore.patterns.command.*;		import uk.ac.sussex.states.LoginGameState;	import uk.ac.sussex.general.ApplicationFacade;	import uk.ac.sussex.model.ServerProxy;		public class SuccessfulCreateUserCommand extends SimpleCommand {				override public function execute (note: INotification ): void {			var server:ServerProxy = facade.retrieveProxy(ServerProxy.NAME) as ServerProxy;			server.logout();					//	var appMediator:ApplicationMediator = facade.retrieveMediator(ApplicationMediator.NAME) as ApplicationMediator;						sendNotification(ApplicationFacade.CHANGE_STATE, LoginGameState.NAME);		}	}	}