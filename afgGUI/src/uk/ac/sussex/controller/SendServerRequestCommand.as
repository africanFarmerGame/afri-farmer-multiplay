package uk.ac.sussex.controller{  import org.puremvc.as3.multicore.interfaces.*;	import org.puremvc.as3.multicore.patterns.command.*;		import uk.ac.sussex.model.ServerProxy;	import uk.ac.sussex.model.valueObjects.RequestData;	  public class SendServerRequestCommand extends SimpleCommand  {		  override public function execute (note: INotification ): void {		  trace("And this sends out the request to the server");			var requestData:RequestData = note.getBody() as RequestData;			if (requestData != null){				trace("request data wasn't null");				var serverProxy:ServerProxy = facade.retrieveProxy(ServerProxy.NAME) as ServerProxy;				serverProxy.sendRequest(requestData);			}		}	  }}