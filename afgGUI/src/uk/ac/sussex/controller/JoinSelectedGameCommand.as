package uk.ac.sussex.controller {	import org.puremvc.as3.multicore.interfaces.*;	import org.puremvc.as3.multicore.patterns.command.*;		import uk.ac.sussex.model.RequestProxy;		public class JoinSelectedGameCommand extends SimpleCommand {		override public function execute ( note:INotification): void {			var gameId:String = note.getBody() as String;			var intGameId:int = int(gameId);						var joinRequestProxy:RequestProxy = facade.retrieveProxy("game.joinGame" + RequestProxy.NAME) as RequestProxy; 			joinRequestProxy.setParamValue("gameId", intGameId);					joinRequestProxy.sendRequest();		}	}	}