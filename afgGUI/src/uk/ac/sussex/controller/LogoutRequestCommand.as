package uk.ac.sussex.controller {	import org.puremvc.as3.multicore.patterns.command.*;	import org.puremvc.as3.multicore.interfaces.*;		import uk.ac.sussex.model.ServerProxy;		public class LogoutRequestCommand extends SimpleCommand {		override public function execute(note:INotification):void {			trace("Attempting to logout (and disconnect?) from the server");			var serverProxy:ServerProxy = facade.retrieveProxy(ServerProxy.NAME) as ServerProxy;			serverProxy.logout();		}	}	}