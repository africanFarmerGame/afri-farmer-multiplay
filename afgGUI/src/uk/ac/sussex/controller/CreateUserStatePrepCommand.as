package uk.ac.sussex.controller {	import org.puremvc.as3.multicore.interfaces.*;	import org.puremvc.as3.multicore.patterns.command.*;		import uk.ac.sussex.view.ApplicationMediator;	import uk.ac.sussex.states.CreateUserGameState;	import uk.ac.sussex.general.ApplicationFacade;		public class CreateUserStatePrepCommand extends SimpleCommand {				override public function execute (note: INotification ): void {			var appMediator:ApplicationMediator = facade.retrieveMediator(ApplicationMediator.NAME) as ApplicationMediator;			var createUserState:CreateUserGameState = new CreateUserGameState(facade);			appMediator.registerState(createUserState);			sendNotification(ApplicationFacade.CHANGE_STATE, CreateUserGameState.NAME);		}	}	}