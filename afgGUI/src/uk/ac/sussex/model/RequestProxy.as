package uk.ac.sussex.model{	import org.puremvc.as3.multicore.interfaces.*;	import org.puremvc.as3.multicore.patterns.proxy.Proxy;	import uk.ac.sussex.model.valueObjects.RequestData;	import uk.ac.sussex.model.valueObjects.requestParams.*;	import uk.ac.sussex.general.ApplicationFacade;		public class RequestProxy extends Proxy implements IProxy	{		public static const NAME:String = "RequestProxy";				public function RequestProxy(requestName:String)		{			super(requestName + NAME, new RequestData(requestName));		}				public function sendRequest():void {			sendNotification(ApplicationFacade.SEND_SERVER_REQUEST, requestData);			requestData.clearValues();	  	}	  	public function addRequestParam(requestParam:DataParam):void {	  		requestData.addRequestParam(requestParam);	  	}	 	public function get requestData():RequestData {	 		return data as RequestData;	  	}	  	public function setParamValue(paramName:String, paramValue:Object):void {			requestData.setParamValue(paramName, paramValue);		}	}}