package uk.ac.sussex.model {	import org.puremvc.as3.multicore.interfaces.IProxy;	import org.puremvc.as3.multicore.patterns.proxy.Proxy;	public class TickerProxy extends Proxy implements IProxy {				public static const NAME:String = "TickerProxy";		public static const TICKER_UPDATED:String = "TickerUpdated";		public function TickerProxy() {			super(NAME, new String());			tickerMessage = "This is a test";		}		public function setTickerMessage(newMessage:String):void {			this.tickerMessage = newMessage;			sendNotification(TICKER_UPDATED, newMessage);		}		public function getTickerMessage():String {			return this.tickerMessage;		}		private function get tickerMessage():String {			return data as String;		}		private function set tickerMessage(newMessage:String):void {			data = newMessage;		}	}	}