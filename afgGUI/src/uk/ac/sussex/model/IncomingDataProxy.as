package uk.ac.sussex.model {	import org.puremvc.as3.multicore.interfaces.*;	import org.puremvc.as3.multicore.patterns.proxy.Proxy;	import uk.ac.sussex.model.valueObjects.IncomingData;	import uk.ac.sussex.model.valueObjects.requestParams.*;		public class IncomingDataProxy extends Proxy implements IProxy{		public static const NAME:String = "IncomingDataProxy";	  	public function IncomingDataProxy(incomingName:String, notification:String){			// constructor code			super(incomingName+NAME, new IncomingData(incomingName, notification));		}		public function handleIncomingData():void {			trace("IncomingDataProxy sez: This should be sending notification: "+incomingData.getNotification());			sendNotification(incomingData.getNotification(), incomingData);		}		public function get incomingData():IncomingData {			return data as IncomingData;		}		public function getParams():Array {			return incomingData.getDataParams();		}		public function addDataParam(dataParam:DataParam):void{	    	incomingData.addDataParam(dataParam);		}	}	}