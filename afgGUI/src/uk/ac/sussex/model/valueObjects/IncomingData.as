package uk.ac.sussex.model.valueObjects {	import uk.ac.sussex.model.valueObjects.requestParams.*;	public class IncomingData {		private var functionName:String;		private var myParams:Array;		private var notification:String;				public function IncomingData(funcName:String, notification:String) {			this.functionName = funcName;			this.notification = notification;			this.myParams = new Array();		}		public function getNotification():String {			return notification;		}		public function getDataParams():Array{			return this.myParams;		}		public function getParamValue(paramName:String):Object {			var param:DataParam = this.myParams[paramName] as DataParam;						if(param != null) {				return param.getParamValue();			}		}		public function addDataParam(dataParam:DataParam):void {	    	var paramName:String = dataParam.getParamName();	    	if(myParams[paramName] == null){  		  		myParams[paramName] = dataParam;			}  		}		public function toString():String {			var retString:String = "IncomingData for " + this.functionName + "\n";			retString += "Containing following params: \n";			for each (var param:DataParam in myParams){				retString += "Param " + param.getParamName();			}			return retString;		}	}	}