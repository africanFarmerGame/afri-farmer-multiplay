package uk.ac.sussex.model.valueObjects {		public class Game {		private var id:int;		private var gameName:String;		private var numPlayers:uint;		private var password:String;				public function Game() {			// constructor code		}		public function getId():int {			return id;		}		public function setId(id:int):void {			this.id = id;		}		public function getName():String {			return this.gameName;		}		public function setName(newName:String):void {			this.gameName= newName;		}		public function getNumberOfPlayers():uint {			return this.numPlayers;		}		public function setNumberOfPlayers(number:uint):void {			this.numPlayers = number;		}		public function setPassword(newPassword:String):void {			this.password = newPassword;		}		public function checkPassword(password:String):Boolean{			return (password == this.password);		}	}	}