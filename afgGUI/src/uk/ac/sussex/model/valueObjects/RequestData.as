package uk.ac.sussex.model.valueObjects{  import uk.ac.sussex.model.valueObjects.requestParams.*;  public class RequestData   {	  private var requestName:String;	  private var myParams:Array;	  	  public function RequestData(requestName:String)	  {		  this.requestName = requestName;		  this.myParams = new Array();	  }	  public function addRequestParam(requestParam:DataParam):void {	    var paramName:String = requestParam.getParamName();	    if(myParams[paramName] == null){  		  myParams[paramName] = requestParam;		  }  	}  	public function getRequestParams():Array {  		return this.myParams;  	}  	public function getRequestName():String {  		return this.requestName;  	}  	public function getParamNames():Array {  		var names:Array = new Array();  		for each (var param:DataParam in this.myParams) {  			names.push(param.getParamName());  		}  		return names;  	}	public function setParamValue(paramName:String, paramValue:Object):void {		var param:DataParam = myParams[paramName];		if(param != null) {			param.setParamValue(paramValue);		}			}	public function clearValues():void {		for each (var param:DataParam in this.myParams) {			param.clearValue();		}	}  }}