package uk.ac.sussex.model.valueObjects.requestParams {	import com.smartfoxserver.v2.entities.data.SFSObject;	import com.smartfoxserver.v2.entities.data.SFSArray;		public class DataParamArrayString extends DataParamArray {		public function DataParamArrayString(paramName:String) {			super(paramName);		}		override public function addToServerParam(existingObject:SFSObject):SFSObject {			var localArray:Array = this.getParamValue() as Array;			var sfsArray:SFSArray = new SFSArray();			for each (var item:String in localArray) {				sfsArray.addUtfString(item);			}			existingObject.putSFSArray(this.getParamName(), sfsArray);			return existingObject;		}		override public function translateFromServerParam(existingObject:SFSObject):SFSObject {			var sfsArray:SFSArray = existingObject.getSFSArray(this.getParamName()) as SFSArray;			if(sfsArray != null){				var sfsSize:int = sfsArray.size();				var myValue:Array = new Array();								for (var i:int = 0; i < sfsSize; i++) {					//trace('In RequestParamArrayString.translateFromServerParam : ' + sfsArray.getUtfString(i));					myValue.push(sfsArray.getUtfString(i));				}			}			this.setParamValue(myValue);			return existingObject;		}			}	}