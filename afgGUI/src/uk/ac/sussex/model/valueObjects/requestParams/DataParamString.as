package uk.ac.sussex.model.valueObjects.requestParams {	import com.smartfoxserver.v2.entities.data.SFSObject;		public class DataParamString extends DataParam {		public function DataParamString(paramName:String) {			super(paramName);			this.paramType = DataParam.STRING;		}		override public function getParamValue() {			var value:String = this.paramValue as String;			return value;		}		override public function setParamValue(paramValue) {			var value:String = paramValue as String;			this.paramValue = value;		}		override public function addToServerParam(existingObject:SFSObject):SFSObject {			existingObject.putUtfString(this.getParamName(), this.getParamValue());			return existingObject;		}		override public function translateFromServerParam(existingObject:SFSObject):SFSObject {			var myValue:String = existingObject.getUtfString(this.getParamName());			this.setParamValue(myValue);			return existingObject;		}	}}