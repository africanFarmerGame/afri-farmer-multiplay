<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
	<class name="uk.ac.sussex.model.DietItem" table="diet_items">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<many-to-one name="diet" column="diet_id" class="uk.ac.sussex.model.Diet" />
 		<many-to-one name="asset" column="asset_id" class="uk.ac.sussex.model.Asset" />
		<property name="amount" column="amount" type="java.lang.Integer" />
	</class>
</hibernate-mapping>